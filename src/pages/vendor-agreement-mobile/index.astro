<script>
// Client-side JavaScript for dynamic vendor data loading
interface VendorData {
  effectiveDate?: string;
  vendorId?: string;
  day?: number;
  month?: number;
  year?: number;
  vendorName?: string;
  vendorAddress?: string;
  vendorEmail?: string;
  vendorPhone?: string;
  status?: string;
  retrievedAt?: string;
	categoryName?: string;
}


document.addEventListener('DOMContentLoaded', async () => {
  const urlParams = new URLSearchParams(window.location.search);
  const vendorId = urlParams.get('vendorId');
  
  if (vendorId) {
    await fetchVendorData(vendorId);
  }
});

async function fetchVendorData(vendorId: string): Promise<void> {
  const loadingContainer = document.getElementById('loading-container');
  
  try {
    // Show loading indicator
    if (loadingContainer) {
      loadingContainer.style.display = 'flex';
    }
    
    const response = await fetch(`/api/vendor-info/${vendorId}`);
    const result = await response.json();
    
    if (result.success && result.data) {
      updateVendorInformation(result.data);
    } else {
      console.error('Failed to fetch vendor data:', result.error);
    }
  } catch (error) {
    console.error('Error fetching vendor data:', error);
  } finally {
    // Hide loading indicator
    if (loadingContainer) {
      loadingContainer.style.display = 'none';
    }
  }
}

function updateVendorInformation(vendorData: VendorData): void {
	if (handleCategoryRouting(vendorData)) {
		return;
	}

  // Update day, month, year - select ALL instances
  const dayElements = document.querySelectorAll('.vendor-day');
  const monthElements = document.querySelectorAll('.vendor-month');
  const yearElements = document.querySelectorAll('.vendor-year');
  const nameElements = document.querySelectorAll('.vendor-name');
  const addressElements = document.querySelectorAll('.vendor-address');
  const emailElements = document.querySelectorAll('.vendor-email');
  const phoneElements = document.querySelectorAll('.vendor-phone');

  if (vendorData.effectiveDate) {
    // Parse the effective date if it exists
    const date = new Date(vendorData.effectiveDate);
    
    // Update ALL day elements
    dayElements.forEach(element => {
      element.textContent = `${date.getDate()}`;
    });
    
    // Update ALL month elements
    monthElements.forEach(element => {
      element.textContent = `${date.toLocaleString('default', { month: 'long' })}`;
    });
    
    // Update ALL year elements
    yearElements.forEach(element => {
      element.textContent = `${date.getFullYear()}`;
    });
  }
  
  // Update ALL vendor name elements
  if (vendorData.vendorName) {
    nameElements.forEach(element => {
      element.textContent = `${vendorData.vendorName}`;
    });
  }

  // Update ALL vendor address elements
  if (vendorData.vendorAddress) {
    addressElements.forEach(element => {
      element.textContent = `${vendorData.vendorAddress}`;
    });
  }

  // Update ALL vendor email elements
  if (vendorData.vendorEmail) {
    emailElements.forEach(element => {
      element.textContent = `${vendorData.vendorEmail}`;
    });
  }

  // Update ALL vendor phone elements
  if (vendorData.vendorPhone) {
    phoneElements.forEach(element => {
      element.textContent = `${vendorData.vendorPhone}`;
    });
  }
}

function handleCategoryRouting(vendorData: VendorData): boolean {
	const categories = extractCategories(vendorData);
	const isPharmacy = categories.some((category) => category.includes('pharmacy'));
	const isOnPharmacyPage = window.location.pathname.includes('/pharmacy-agreement-mobile');

	if (isPharmacy && !isOnPharmacyPage) {
		window.location.href = `/pharmacy-agreement-mobile${window.location.search}`;
		return true;
	}

	if (!isPharmacy && isOnPharmacyPage) {
		window.location.href = `/vendor-agreement-mobile${window.location.search}`;
		return true;
	}

	return false;
}

function extractCategories(vendorData: VendorData): string[] {
	const collected: Array<string | undefined | null | string[]> = [
		vendorData.categoryName,
	];

	const flattened = collected.flatMap((entry) => {
		if (!entry) {
			return [];
		}

		if (Array.isArray(entry)) {
			return entry;
		}

		return `${entry}`.split(/[;,]/);
	});

	return flattened
		.map((value) => value.trim().toLowerCase())
		.filter((value) => value.length > 0);
}
</script>
