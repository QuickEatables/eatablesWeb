<script>
	interface RiderData {
		riderId?: string;
		effectiveDate?: string;
		day?: number;
		month?: number;
		year?: number;
		riderName?: string;
		riderEmail?: string;
		riderPhone?: string;
		status?: string;
        state?: string;
        stateFirstLetter?: string;
		retrievedAt?: string;
	}

	document.addEventListener('DOMContentLoaded', async () => {
		const urlParams = new URLSearchParams(window.location.search);
		const riderId = urlParams.get('riderId');

		if (riderId) {
			await fetchRiderData(riderId);
		}
	});

	async function fetchRiderData(riderId: string): Promise<void> {
		const loadingContainer = document.getElementById('loading-container');

		try {
			if (loadingContainer) {
				loadingContainer.style.display = 'flex';
			}

			const response = await fetch(`/api/rider-info/${encodeURIComponent(riderId)}`);
			const result = await response.json();

			if (result.success && result.data) {
				updateRiderInformation(result.data);
			} else {
				console.error('Failed to fetch rider data:', result.error);
			}
		} catch (error) {
			console.error('Error fetching rider data:', error);
		} finally {
			if (loadingContainer) {
				loadingContainer.style.display = 'none';
			}
		}
	}

	function updateRiderInformation(riderData: RiderData): void {
		const { dayString, monthName, uppercaseMonth, yearString } = deriveDateParts(riderData);

		replacePlaceholder('(RIDER NAME)', riderData.riderName);
		replacePlaceholder('(Rider/Logistics name)', riderData.riderName);

		if (dayString && monthName && yearString) {
			replacePlaceholder('(DAY), (MONTH), (YEAR)', `${dayString}, ${monthName}, ${yearString}`);
		}

		if (dayString && yearString) {
			replacePlaceholder('(Day),(Year)', `${dayString}, ${yearString}`);
		}

		replacePlaceholder('(DAY)', dayString);
		replacePlaceholder('(Day)', dayString);
		replacePlaceholder('(Date)', dayString);

		replacePlaceholder('(MONTH)', uppercaseMonth);
		replacePlaceholder('(Month)', monthName);
		replacePlaceholder('(Month in words)', monthName);

		replacePlaceholder('(YEAR)', yearString);
		replacePlaceholder('(Year)', yearString);

        replacePlaceholder('(State)', riderData.state);
        replacePlaceholder('(State first letter)', riderData.stateFirstLetter);
	}

	function deriveDateParts(riderData: RiderData): {
		dayString?: string;
		monthName?: string;
		uppercaseMonth?: string;
		yearString?: string;
	} {
		let date: Date | undefined;

		if (riderData.effectiveDate) {
			const parsed = new Date(riderData.effectiveDate);
			if (!Number.isNaN(parsed.valueOf())) {
				date = parsed;
			}
		}

		const day = typeof riderData.day === 'number' ? riderData.day : date?.getDate();
		const month =
			typeof riderData.month === 'number'
				? riderData.month
				: date
					? date.getMonth() + 1
					: undefined;
		const year =
			typeof riderData.year === 'number'
				? riderData.year
				: date?.getFullYear();

		const dayString = day !== undefined ? String(day) : undefined;

		let monthName: string | undefined;
		if (month !== undefined) {
			const normalizedMonth = Math.max(1, Math.min(12, month));
			monthName = new Date(2000, normalizedMonth - 1, 1).toLocaleString('default', { month: 'long' });
		} else if (date) {
			monthName = date.toLocaleString('default', { month: 'long' });
		}

		const uppercaseMonth = monthName ? monthName.toUpperCase() : undefined;
		const yearString = year !== undefined ? String(year) : undefined;

		return { dayString, monthName, uppercaseMonth, yearString };
	}

	function replacePlaceholder(placeholder: string, value?: string): void {
		if (!value) {
			return;
		}

		const container = document.querySelector('.rider-agreement-content');
		if (!container) {
			return;
		}

		const walker = document.createTreeWalker(container, NodeFilter.SHOW_TEXT);
		const textNodes: Text[] = [];

		while (walker.nextNode()) {
			const node = walker.currentNode as Text;
			if (node.nodeValue && node.nodeValue.includes(placeholder)) {
				textNodes.push(node);
			}
		}

		textNodes.forEach((node) => {
			if (node.nodeValue) {
				node.nodeValue = node.nodeValue.replaceAll(placeholder, value);
			}
		});
	}
	</script>